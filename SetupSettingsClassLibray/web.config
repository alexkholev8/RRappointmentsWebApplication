  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  
	<configSections>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="RRappointmentsWebApplication.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
	</configSections>
  
	<system.web>
		<compilation debug="true" targetFramework="4.0">
			<assemblies>
				<!--<add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>-->
				<add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
				<add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
			</assemblies>
		</compilation>
		<httpHandlers>
			<add verb="*" path="CaptchaImage.axd" type="MSCaptcha.CaptchaImageHandler, MSCaptcha"/>
		</httpHandlers>
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
	</system.web>
  
	<location path="CaptchaImage.axd">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
  
	<appSettings>
    <add key="WebServiceIP" value="192.168.0.200"/> <!-- // value="212.68.132.60"-->
    <add key="WebServicePort" value=":80"/>         <!-- // value=":446" or ":80" or ":443" -->
		<add key="LogoImage" value="images/logo.png"/>
		<add key="EnableTreatmentGroups" value="false"/>
		<add key="AppointmentLimit" value="0"/>
    <add key="wsUserName" value="iisclient" />
    <add key="wsPassword" value="123" />
    <add key="wsDBname" value="medical"/>
    <add key="ajaxTransferingMode" value="Object"/> <!--// added on 23/06/2013 -->
    <!-- // added on 18/07/2013 -->
    <add key="WebServiceLocation" value="0"/>
    <add key="WebServiceDirectory" value="WebServices/"/>
    <!-- // end added -->
	</appSettings>
  
	<system.serviceModel>
		<bindings/>
		<client/>
	</system.serviceModel>

  <applicationSettings>
    <RRappointmentsWebApplication.Properties.Settings>
      <setting name="WebServicePort" serializeAs="String">
        <value>:80</value>            <!-- :446 or :80 or :443 -->
      </setting>
      <setting name="WebServiceIP" serializeAs="String">
        <value>192.168.0.200</value>  <!--212.68.132.60-->
      </setting>
      <setting name="WebServiceLocation" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="WebServiceDirectory" serializeAs="String">
        <value>WebServices/</value>
      </setting>
    </RRappointmentsWebApplication.Properties.Settings>
  </applicationSettings>
  
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false"/>
		<directoryBrowse enabled="true"/>
	</system.webServer>
  
</configuration>
